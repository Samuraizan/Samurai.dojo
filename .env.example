# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/samuraidojo

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret

# API Keys (for production)
OPENAI_API_KEY=your-openai-api-key
DEEPSEEK_API_KEY=your-deepseek-api-key

# Quantum Configuration
QUANTUM_PROCESSOR_ENABLED=true
QUANTUM_REGISTER_SIZE=8
QUANTUM_ENTANGLEMENT_THRESHOLD=0.85

# Performance
EDGE_CONFIG={"imageSizes":[64,128,256,384],"deviceSizes":[640,768,1024,1280,1920]}
CACHE_CONTROL_HEADER=public, s-maxage=3600, stale-while-revalidate=7200

# Monitoring
SENTRY_DSN=your-sentry-dsn
AXIOM_TOKEN=your-axiom-token
VERCEL_ENV=development 